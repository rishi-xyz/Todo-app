{
  "projects": {
    "frontend": {
      "src": "frontend/",
      "buildCommand": "npm run build",
      "outputDirectory": "dist"
    },
    "backend": {
      "src": "backend/",
      "buildCommand": "npm install",
      "outputDirectory": "."
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}

